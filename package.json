{
  "name": "@fullstackio/cq",
  "version": "1.0.1",
  "description": "query code with selectors",
  "main": "dist/index.js",
  "repository": "fullstackio/cq",
  "scripts": {
    "compile": "babel -d dist/ src/",
    "prepublish": "npm run generate-parser && npm run compile",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register",
    "watchtest": "./node_modules/.bin/mocha --compilers js:babel-core/register --watch --watch-extensions pegjs",
    "generate-parser": "./node_modules/.bin/pegjs src/query.pegjs src/query-parser.js"
  },
  "bin": {
    "cq": "dist/cli.js"
  },
  "author": "Nate Murray <nate@fullstack.io>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "babel-traverse": "^6.9.0",
    "babel-types": "^6.10.0",
    "babylon": "^6.8.1",
    "typescript": "^1.8.10",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-syntax-from-presets": "^1.0.1",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "pegjs": "^0.9.0"
  }
}
